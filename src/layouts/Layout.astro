---
import Navigation from "../components/Navigation.astro";
import "../styles/global.css";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="description" content="LEEWONJAE" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <title>{title}</title>
  </head>
  <body>
    <Navigation />
    <slot />
  </body>
</html>
